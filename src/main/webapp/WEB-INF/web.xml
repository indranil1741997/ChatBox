<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
		 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
		 					http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
<welcome-file-list>
	<welcome-file>index.jsp</welcome-file>
</welcome-file-list>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<servlet-class>com.chatbox.servlet.LoginServlet</servlet-class>
  </servlet>
 <servlet>
  	<servlet-name>RegisterServlet</servlet-name>
  	<servlet-class>com.chatbox.servlet.RegisterServlet</servlet-class>
  </servlet>
 <servlet>
 	<servlet-name>UserProfile</servlet-name>
 	<servlet-class>com.tobeformatted.UserProfile</servlet-class>
 </servlet>
 <servlet>
 	<servlet-name>LogoutServlet</servlet-name>
 	<servlet-class>com.chatbox.servlet.LogoutServlet</servlet-class>
 </servlet>
 <servlet>
 	<servlet-name>MessageServlet</servlet-name>
 	<servlet-class>com.chatbox.servlet.MessageServlet</servlet-class>
 </servlet>
 <servlet>
 	<servlet-name>AddMessage</servlet-name>
 	<servlet-class>com.connector.AddMessage</servlet-class>
 </servlet>
  <servlet-mapping>
  	<servlet-name>RegisterServlet</servlet-name>
  	<url-pattern>/Register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/Login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UserProfile</servlet-name>
  	<url-pattern>/UserProfile</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LogoutServlet</servlet-name>
  	<url-pattern>/Logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MessageServlet</servlet-name>
  	<url-pattern>/message</url-pattern>
  </servlet-mapping>
</web-app>
